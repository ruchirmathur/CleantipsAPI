{
  "taskDefinition": {
    "family": "CleantipsTask",
    "containerDefinitions": [
      {
        "name": "cleantips/cleantipsapi",
        "image": "$AWS_ACCOUNT_ID.dkr.ecr.us-west-2.amazonaws.com/cleantips/cleantipsapi",
        "cpu": 100,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "memory": 128,
        "essential": true
      }
    ]
  }
}